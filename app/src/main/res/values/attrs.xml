<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RefreshView">
        <attr name="enableLoadMore" format="boolean"/>
        <attr name="enableRefresh" format="boolean"/>
        <attr name="showNoData" format="boolean"/>
        <attr name="showLoading" format="boolean"/>
        <attr name="layout" format="reference"/>
    </declare-styleable>
    <declare-styleable name="RateImageView">
        <attr name="rate" format="float"/>
        <attr name="isWidthRate" format="boolean"/>

    </declare-styleable>
    <declare-styleable name="SwitchButton">
        <attr name="select" format="boolean"/>
        <attr name="select_image" format="reference"/>
        <attr name="select_text" format="string"/>
        <attr name="unselect_image" format="reference"/>
        <attr name="unselect_text" format="string"/>
        <attr name="select_background_color" format="color"/>
        <attr name="unselect_background_color" format="color"/>
        <attr name="select_text_color" format="color"/>
        <attr name="unselect_text_color" format="color"/>
        <attr name="image_size" format="dimension"/>
        <attr name="text_size" format="integer"/>
        <attr name="interval" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="CutCardView">
        <attr name="AllCut" format="boolean"/>
        <attr name="TopLeftCut" format="boolean"/>
        <attr name="TopRightCut" format="boolean"/>
        <attr name="BottomLeftCut" format="boolean"/>
        <attr name="BottomRightCut" format="boolean"/>
        <attr name="CutElevation" format="dimension"/>
        <attr name="TopLeftCutRadius" format="dimension"/>
        <attr name="TopRightCutRadius" format="dimension"/>
        <attr name="BottomLeftRadius" format="dimension"/>
        <attr name="BottomRightRadius" format="dimension"/>
        <attr name="AllCutRadius" format="dimension"/>
        <attr name="CutForeground" format="reference"/>
        <attr name="CutProgress" format="integer"/>
    </declare-styleable>
</resources>